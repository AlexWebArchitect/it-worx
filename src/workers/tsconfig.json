{
  "compilerOptions": {
    "sourceMap": false,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "removeComments": true,
    "strictNullChecks": false,
    "outDir": "build",
    "lib": [
      "es2015",
      "dom"
    ]
  },
  "files": ["worker.ts"],
  "exclude": [
    "dist",
    "build",
    "node_modules",
    "components"
  ]
}